{
  "name": "@solvyst/forms-engine-express",
  "version": "0.1.0",
  "description": "Express adapter for @solvyst/forms-engine-core.",
  "license": "MIT",
  "author": "Anil Moharana",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@solvyst/forms-engine-core": "^0.1.0"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  }
}
